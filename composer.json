{
    "name": "chronon/pawsback",
    "description": "A PHP backup solution using S3",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregory Gaskill",
            "email": "gregory@chronon.com"
        }
    ],
    "support": {
        "issues": "https://github.com/chronon/pawsback/issues",
        "source": "https://github.com/chronon/pawsback"
    },
    "require": {
        "php": ">=7.0.0",
        "ext-curl": "*",
        "aws/aws-sdk-php": "^3.25"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {"Pawsback\\": "src/Pawsback"}
    },
    "autoload-dev": {
        "psr-4": {"Pawsback\\Test\\": "tests/Pawsback"}
    },
    "bin": ["bin/pawsback"]
}
